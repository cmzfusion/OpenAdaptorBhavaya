    <information>
        <title>Bhavaya Always on Top Demo</title>
        <vendor>Bhavaya</vendor>
        <homepage href="http://bhavaya.openadaptor.org"/>
        <description>Bhavaya Always on Top Demo</description>
	<offline-allowed/>
    </information>

    <resources>
        <j2se version="1.4.1* 1.4" max-heap-size="256m"/>
        <property name="sun.java2d.noddraw" value="true"/>

        <jar href="bhavaya.jar" main="true" download="eager"/>
        <jar href="log4j-1.2.15-patched.jar" download="eager"/>
        <nativelib href="bhavayaNativeLib.jar" download="eager"/>
    </resources>

    <security>
        <all-permissions/>
    </security>

    <application-desc main-class="org.bhavaya.ui.AlwaysOnTopDemo"/>

</jnlp>
